set fileencodings=utf-8,ucs-bom,gb18030,gbk,gb2312,cp936
set cursorline
syntax on
filetype on
filetype plugin on
filetype indent on
set autoindent
set ai!
set incsearch
set nocp
set termencoding=utf-8
set encoding=utf-8
set number
set hlsearch
set showmatch
set mouse=a
set selection=exclusive
set selectmode=mouse,key
set tabstop=4
set paste
set listchars=tab:>-,trail:-
set ruler
set whichwrap=b,s,<,>,[,]
set spell
